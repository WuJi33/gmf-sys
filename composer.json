{
  "name": "ggoop/gmf-sys",
  "description": "gmf for laravel5",
  "license": "MIT",
  "authors": [
  {
    "name": "ggoop",
    "email": "samuel_abe@163.com"
  }],
  "require":
  {
    "php": ">=5.6.4",
    "predis/predis": "^1.1",
    "illuminate/support": "~5.5",
    "illuminate/auth": "~5.5",
    "illuminate/http": "~5.5",
    "illuminate/console": "~5.5",
    "illuminate/database": "~5.5",
    "illuminate/contracts": "~5.5",
    "guzzlehttp/guzzle": "~6.0",
    "simplesoftwareio/simple-qrcode": "~2",
    "firebase/php-jwt": "~3.0|~4.0",
    "league/oauth2-server": "^6.0",
    "phpseclib/phpseclib": "^2.0",
    "hisorange/browser-detect": "^3.1",
    "elasticsearch/elasticsearch": "~5.0",
    "maatwebsite/excel": "^3.0",
    "symfony/psr-http-message-bridge": "~1.0",
    "zendframework/zend-diactoros": "~1.0",
    "doctrine/dbal": "^2.5",
    "fzaninotto/faker": "~1.4"
  },
  "require-dev":
  {},
  "autoload":
  {
    "psr-4":
    {
      "Gmf\\Sys\\": "src/"
    }
  },
  "extra":
  {
    "laravel":
    {
      "providers": [
        "Gmf\\Sys\\ServiceProvider"
      ],
      "aliases":
      {
        "Uuid": "Gmf\\Sys\\Facades\\Uuid",
        "GAuth": "Gmf\\Sys\\Facades\\GAuth",
        "Packager": "Gmf\\Sys\\Facades\\Packager"
      }
    }
  },
  "minimum-stability": "dev"
}
