{
  "name": "ggoop/gmf-sys",
  "description": "gmf for laravel5",
  "license": "MIT",
  "authors": [
  {
    "name": "ggoop",
    "email": "samuel_abe@163.com"
  }],
  "require":{
    "php": ">=5.6.4",
    "predis/predis": "^1.1",
    "illuminate/support": "~5.5",
    "illuminate/auth": "~5.5",
    "illuminate/http": "~5.5",
    "illuminate/console": "~5.5",
    "illuminate/database": "~5.5",
    "illuminate/contracts": "~5.5",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "simplesoftwareio/simple-qrcode": "^1.5"
  },
  "autoload":
  {
    "psr-4":
    {
      "Gmf\\Sys\\": "src/"
    }
  },
  "extra":
  {
    "laravel":
    {
      "providers": [
        "Gmf\\Sys\\ServiceProvider"
      ],
      "aliases":
      {
        "Uuid": "Gmf\\Sys\\UuidFacade",
        "Ent": "Gmf\\Sys\\EntFacade"
      }
    }
  },
  "minimum-stability": "dev"
}
